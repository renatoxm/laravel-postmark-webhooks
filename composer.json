{
    "name": "renatoxm/laravel-postmark-webhooks",
    "description": "Handle Postmark webhooks in a Laravel application.",
    "keywords": [
        "Laravel",
        "Postmark",
        "webhook",
        "e-mail"
    ],
    "homepage": "https://github.com/renatoxm/laravel-postmark-webhooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Renato XM",
            "email": "renatoxm@gmail.com",
            "homepage": "https://github.com/renatoxm",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0 || ^11.0",
        "phpstan/extension-installer": "^1.3"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^7.0 || ^8.17 || ^9.0",
        "phpunit/phpunit": "^9.4 || ^10.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0-dev",
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "RenatoXM\\PostmarkWebhooks\\PostmarkWebhooksServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "RenatoXM\\PostmarkWebhooks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenatoXM\\PostmarkWebhooks\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "format": "php-cs-fixer fix --allow-risky=yes",
        "analyse": "phpstan analyse"
    }
}
